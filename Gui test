Title.TextColor3 = Color3.fromRGB(255,80,80)

local Features = Instance.new("TextLabel", Left)
Features.Size = UDim2.new(1,-20,1,-70)
Features.Position = UDim2.new(0,10,0,60)
Features.BackgroundTransparency = 1
Features.TextXAlignment = Enum.TextXAlignment.Left
Features.TextYAlignment = Enum.TextYAlignment.Top
Features.Font = Enum.Font.Gotham
Features.TextSize = 16
Features.TextColor3 = Color3.fromRGB(255,255,255)
Features.Text = "üéÅ Giveaways\n‚ö° Features\nüõ° Safe\nüëë SL Team"

-- RIGHT
local Right = Instance.new("Frame", Main)
Right.Size = UDim2.new(0.55,0,1,0)
Right.Position = UDim2.new(0.45,0,0,0)
Right.BackgroundTransparency = 1

local Box = Instance.new("TextBox", Right)
Box.Size = UDim2.new(0.8,0,0,45)
Box.Position = UDim2.new(0.1,0,0.2,0)
Box.PlaceholderText = "Enter Key..."
Box.BackgroundColor3 = Color3.fromRGB(25,25,25)
Box.TextColor3 = Color3.fromRGB(255,255,255)
Instance.new("UICorner", Box)

local Verify = Instance.new("TextButton", Right)
Verify.Size = UDim2.new(0.8,0,0,45)
Verify.Position = UDim2.new(0.1,0,0.35,0)
Verify.Text = "VERIFY"
Verify.BackgroundColor3 = Color3.fromRGB(255,0,0)
Verify.TextColor3 = Color3.fromRGB(255,255,255)
Instance.new("UICorner", Verify)

-- üîò AntiAFK (ÿ®ÿØŸàŸÜ key)
local AntiBtn = Instance.new("TextButton", Right)
AntiBtn.Size = UDim2.new(0.8,0,0,45)
AntiBtn.Position = UDim2.new(0.1,0,0.5,0)
AntiBtn.Text = "LOAD ANTI AFK"
AntiBtn.BackgroundColor3 = Color3.fromRGB(255,0,0)
AntiBtn.TextColor3 = Color3.fromRGB(255,255,255)
Instance.new("UICorner", AntiBtn)

AntiBtn.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://your-antiafk-script-link.lua"))()
    AntiBtn.Text = "ANTI AFK LOADED ‚úÖ"
    AntiBtn.BackgroundColor3 = Color3.fromRGB(0,170,0)
end)

-- ÿ≥ŸÉÿ±ÿ®ÿ™ÿßÿ™ ŸÉŸÜÿµ
local Scripts = Instance.new("Frame", Right)
Scripts.Size = UDim2.new(1,0,0.5,0)
Scripts.Position = UDim2.new(0,0,0.2,0)
Scripts.BackgroundTransparency = 1
Scripts.Visible = false

local function NewScript(name,pos,link)
    local txt = Instance.new("TextButton", Scripts)
    txt.Size = UDim2.new(1,0,0,35)
    txt.Position = UDim2.new(0,0,0,pos)
    txt.BackgroundTransparency = 1
    txt.Text = name
    txt.TextColor3 = Color3.fromRGB(255,80,80)
    txt.Font = Enum.Font.GothamBold
    txt.TextSize = 18

    txt.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet(link))()
    end)
end

NewScript("Muscle Legends Script",0,"ML_SCRIPT_LINK")
NewScript("Blox Fruits Script",0.15,"BF_SCRIPT_LINK")
NewScript("Legends Of Speed Script",0.30,"LS_SCRIPT_LINK")
NewScript("Escape Tsunami Script",0.45,"ET_SCRIPT_LINK")

-- KEY
local CorrectKey = "SL-ML-2026"

Verify.MouseButton1Click:Connect(function()
    if Box.Text == CorrectKey then
        Verify.Visible = false
        Box.Visible = false
        Features.Visible = false
        Title.Text = "üî• SCRIPTS üî•"
        Scripts.Visible = true
    end
end)
