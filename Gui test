Features.Text =
"‚ö° FEATURES\nüõ° SAFE\nüëë GOOD SCRIPTS\nüö´ ANTIBAN\nüöÄ FAST EXECUTION\nüí° EASY TO USE\nüìâ NO LAG\n\nüëª MADE BY SL_GHOST"

-- RGB EFFECT
task.spawn(function()
    while true do
        for i = 0,255,5 do
            Features.TextColor3 = Color3.fromHSV(i/255,1,1)
            task.wait()
        end
    end
end)

-- RIGHT
local Right = Instance.new("Frame", Main)
Right.Size = UDim2.new(0.55,0,1,0)
Right.Position = UDim2.new(0.45,0,0,0)
Right.BackgroundTransparency = 1

-- Key box
local Box = Instance.new("TextBox", Right)
Box.Size = UDim2.new(0.8,0,0,45)
Box.Position = UDim2.new(0.1,0,0.15,0)
Box.PlaceholderText = "Paste key here..."
Box.BackgroundColor3 = Color3.fromRGB(25,25,25)
Box.TextColor3 = Color3.fromRGB(255,255,255)
Instance.new("UICorner", Box)

-- Verify button
