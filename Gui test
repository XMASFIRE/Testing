-- Anti AFK button
local AntiBtn = Instance.new("TextButton", Right)
AntiBtn.Size = UDim2.new(0.8,0,0,45)
AntiBtn.Position = UDim2.new(0.1,0,0.54,0)
AntiBtn.Text = "LOAD ANTI AFK"
AntiBtn.BackgroundColor3 = Color3.fromRGB(255,0,0)
AntiBtn.TextColor3 = Color3.fromRGB(255,255,255)
Instance.new("UICorner", AntiBtn)

AntiBtn.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("ANTI_AFK_RAW_LINK"))()
    AntiBtn.Text = "ANTI AFK LOADED ‚úÖ"
    AntiBtn.BackgroundColor3 = Color3.fromRGB(0,170,0)
end)

-- Scripts Frame
local Scripts = Instance.new("Frame", Right)
Scripts.Size = UDim2.new(0.8,0,0.35,0)
Scripts.Position = UDim2.new(0.1,0,0.68,0)
Scripts.BackgroundTransparency = 1
Scripts.Visible = false

local layout = Instance.new("UIListLayout", Scripts)
layout.Padding = UDim.new(0,5)

local function NewScript(name,link)
    local btn = Instance.new("TextButton", Scripts)
    btn.Size = UDim2.new(1,0,0,35)
    btn.BackgroundColor3 = Color3.fromRGB(255,0,0)
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    btn.Text = name
    Instance.new("UICorner", btn)

    btn.MouseButton1Click:Connect(function()
        Gui.Enabled = false
        loadstring(game:HttpGet(link))()
    end)
end


-- GAME SCRIPTS
NewScript("Muscle Legends","https://raw.githubusercontent.com/XMASFIRE/SL-HUB-D/refs/heads/main/SL%20HUB%20ML%20(1).txt")
NewScript("Blox Fruits","https://raw.githubusercontent.com/XMASFIRE/SL-HUB-D/refs/heads/main/BloxFruits")
NewScript("Legends Of Speed","https://raw.githubusercontent.com/XMASFIRE/SL-HUB-D/refs/heads/main/SL%20HUB%20LOS")
NewScript("Escape Tsunami","https://raw.githubusercontent.com/XMASFIRE/SL-HUB-D/refs/heads/main/Tusnammi.txt")
NewScript("Anti AFK","https://raw.githubusercontent.com/XMASFIRE/SL-HUB-D/refs/heads/main/SL%20HUB%20ANTI%20AFK")
-- Key system
local CorrectKey = "SLONTOP"
Verify.MouseButton1Click:Connect(function()
    if Box.Text == CorrectKey then
        Box.Visible = false
        Verify.Visible = false
        GetKey.Visible = false
        Scripts.Visible = true
    end
end)
