repeat task.wait() until game:IsLoaded()

local CorrectKey = "1234"

-- GUI
local ScreenGui = Instance.new("ScreenGui", game.CoreGui)

-- KEY FRAME
local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0,300,0,180)
Frame.Position = UDim2.new(0.5,-150,0.5,-90)
Frame.Active = true
Frame.Draggable = true

local Box = Instance.new("TextBox", Frame)
Box.Size = UDim2.new(0,260,0,40)
Box.Position = UDim2.new(0,20,0,20)
Box.PlaceholderText = "Paste key here"

local Check = Instance.new("TextButton", Frame)
Check.Size = UDim2.new(0,120,0,40)
Check.Position = UDim2.new(0,20,0,80)
Check.Text = "Check Key"

local GetKey = Instance.new("TextButton", Frame)
GetKey.Size = UDim2.new(0,120,0,40)
GetKey.Position = UDim2.new(0,160,0,80)
GetKey.Text = "Get Key"

-- HUB
local Hub = Instance.new("Frame", ScreenGui)
Hub.Size = UDim2.new(0,300,0,260)
Hub.Position = UDim2.new(0.5,-150,0.5,-130)
Hub.Visible = false
Hub.Active = true
Hub.Draggable = true

local UIList = Instance.new("UIListLayout", Hub)
UIList.Padding = UDim.new(0,6)

-- BUTTON MAKER
local function CreateBtn(name, raw)
    local b = Instance.new("TextButton", Hub)
    b.Size = UDim2.new(1,0,0,40)
    b.Text = name
    b.MouseButton1Click:Connect(function()
        b.Text = "Loading..."
        loadstring(game:HttpGet(raw))()
        b.Text = name.." Loaded"
    end)
end

-- ANTI AFK (no key)
local Anti = Instance.new("TextButton", Hub)
Anti.Size = UDim2.new(1,0,0,40)
Anti.Text = "Load Anti AFK"
Anti.MouseButton1Click:Connect(function()
    local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:Connect(function()
        vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        task.wait(1)
        vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
    Anti.Text = "Anti AFK Loaded"
end)

-- SCRIPTS
CreateBtn("Legends Of Speed","RAW1")
CreateBtn("Muscle Legends","RAW2")
CreateBtn("Blox Fruits","RAW3")
CreateBtn("Escape Tsunami","RAW4")

-- CHECK KEY
Check.MouseButton1Click:Connect(function()
    if Box.Text == CorrectKey then
        Frame.Visible = false
        Hub.Visible = true
    else
        Check.Text = "Wrong Key"
    end
end)

-- GET KEY
GetKey.MouseButton1Click:Connect(function()
    setclipboard("https://yourkeylink.com")
    GetKey.Text = "Copied"
end)
