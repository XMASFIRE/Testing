GetKey.Position = UDim2.new(0.1,0,0.42,0)
GetKey.Text = "GET KEY"
GetKey.BackgroundColor3 = Color3.fromRGB(255,0,0)
GetKey.TextColor3 = Color3.fromRGB(255,255,255)
Instance.new("UICorner", GetKey)

GetKey.MouseButton1Click:Connect(function()
    setclipboard("SL-ML-2026") -- ÿ∂ÿπ ŸáŸÜÿß ÿ±ÿßÿ®ÿ∑ ÿßŸÑŸÖŸÅÿ™ÿßÿ≠ ÿ•ÿ∞ÿß ÿ≠ÿßÿ®ÿ®
