-- KEY
local CorrectKey = "SL-ML-2026"

Verify.MouseButton1Click:Connect(function()
    if Box.Text == CorrectKey then
        Verify.Text = "KEY CORRECT ‚úÖ"
        Verify.BackgroundColor3 = Color3.fromRGB(0,170,0)
        Games.Visible = true
        GetKey.Visible = false
        AntiBtn.Visible = true
    else
        Verify.Text = "WRONG KEY ‚ùå"
        Verify.BackgroundColor3 = Color3.fromRGB(170,0,0)
    end
end)

-- GET KEY BUTTON
GetKey.MouseButton1Click:Connect(function()
    setclipboard("https://your-key-link.com")
end)

-- ANTI AFK LOADSTRING
AntiBtn.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://your-antiafk-script-link.lua"))()
    AntiBtn.Text = "ANTI AFK LOADED ‚úÖ"
    AntiBtn.BackgroundColor3 = Color3.fromRGB(0,170,0)
end)

